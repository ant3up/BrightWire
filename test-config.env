# Google Sheets Test Configuration
# Copy this to .env and fill in your actual values

USE_GOOGLE_SHEETS=true
GOOGLE_SHEET_ID=your_google_sheet_id_here
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
GOOGLE_WORKSHEET_NAME=Sheet1

# GitHub Configuration (required for full test)
GITHUB_TOKEN=ghp_your_github_token_here
GITHUB_OWNER=your-username
GITHUB_REPO=your-repo-name
GITHUB_BASE_BRANCH=main

# Vercel Configuration (required for full test)
VERCEL_TOKEN=your_vercel_token_here
VERCEL_PROJECT_ID=your_project_id
VERCEL_TEAM_ID=your_team_id
SITE_BASE_URL=https://your-site.vercel.app

# Site Configuration
SITE_FILE_PATHS=["public/site-config.json","src/config/site.json","src/config/constants.ts"]

# Script Configuration
DRY_RUN=true
MAX_AGE_DAYS=30
